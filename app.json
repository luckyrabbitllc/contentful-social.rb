{
    "name": "SitePress",
    "description": "A beautiful & open-source content, marketing, and analytics platform for startups.",
    "keywords": [
        "CMS",
        "Blog",
        "Marketing",
        "Laravel",
        "Contentful"
    ],
    "website": "https://sitepress.herokuapp.com",
    "repository": "https://github.com/luckyrabbitllc/SitePress",
    "logo": "https://images.contentful.com/x5o3atz1wqhm/2PWSbcsefYImQyMuqcIuGi/5efaa2c98a4819ef729885a7c3aa381c/App_Icon_2x.png",
    "success_url": "/",
    "env": {
        "RAILS_ENV": "production",
        "COOKIE_SECRET": {
            "description": "This gets generated",
            "generator": "secret"
        },
        "SETUP_BY": {
            "description": "Who initiated this setup",
            "value": ""
        }
    },
    "scripts": {
        "postdeploy": "bundle exec rake db:migrate"
    },
    "formation": {
        "web": {
            "quantity": 1,
            "size": "Free"
        }
    },
    "image": "heroku/php",
    "addons": [
        {
            "plan": "heroku-postgresql",
            "options": {
                "version": "9.5"
            }
        }
    ],
    "buildpacks": [
        {
            "url": "https://github.com/heroku/heroku-buildpack-ruby"
        }
    ],
    "environments": {
        "test": {
            "scripts": {
                "test": "bundle exec rake test"
            }
        }
    }
}